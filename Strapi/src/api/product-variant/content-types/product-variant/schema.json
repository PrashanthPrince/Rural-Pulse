{
  "kind": "collectionType",
  "collectionName": "product_variants",
  "info": {
    "singularName": "product-variant",
    "pluralName": "product-variants",
    "displayName": "product-variant"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "variant_name": {
      "type": "string",
      "required": true
    },
    "variety_type": {
      "type": "string"
    },
    "grade": {
      "type": "string"
    },
    "brand": {
      "type": "string"
    },
    "weight": {
      "type": "decimal"
    },
    "weight_unit": {
      "type": "enumeration",
      "enum": [
        "kg",
        "g",
        "L",
        "ml"
      ]
    },
    "size": {
      "type": "string"
    },
    "style": {
      "type": "string"
    },
    "package_type": {
      "type": "string"
    },
    "stock_qty": {
      "type": "integer"
    },
    "Store_keeping_Unit": {
      "type": "string"
    },
    "barcode": {
      "type": "string"
    },
    "product_ids": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "mappedBy": "product_variants"
    },
    "varientImage": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "isDefault": {
      "type": "boolean",
      "default": false
    },
    "sortOrder": {
      "type": "integer"
    }
  }
}
